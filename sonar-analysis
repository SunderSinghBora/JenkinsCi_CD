node{
   stage('SCM Checkout'){
     git 'https://github.com/SunderSinghBora/JenkinsCi_CD'
   }
   stage('Compile-Package'){
      
	  bat label: '', script: 'mvn clean package'
   }
   
   stage('SonarQube Analysis') {
       
	   bat label: '', script: 'mvn sonar:sonar'
        }
    }
   


}
